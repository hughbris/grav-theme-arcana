{% if form.title %}
<h3>{{ form.title }}</h3>
{% endif %}
<form
    {% block embed_form_core %}{% endblock %}
    {% block embed_form_classes %}{% endblock %}
    {% block embed_form_custom_attributes %}{% endblock %}
>
<div class="row gtr-50">
  {% block embed_fields %}{% endblock %}
  <div class="col-12">
    {% block embed_buttons %}{% endblock %}
  </div>
</div>
</form>
