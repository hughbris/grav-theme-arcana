{% extends 'default.html.twig' %}

{% block body %}
<!-- Main -->
	<section class="wrapper style1">
		<div class="container">
			<div class="row gtr-200">
				<div class="col-3 col-12-narrower">
					{{ include('partials/sidebar.html.twig', {'sid': 'sidebar1'}) }}
				</div>

				<div class="col-6 col-12-narrower imp-narrower">

					<div id="content">

						<!-- Content -->

							<article>
								{% block precis %}
									{{ parent() }}
								{% endblock %}

								{% block feature %}
									{{ parent() }}
								{% endblock %}

								{% block content %}
									{{ page.content|raw }}
								{% endblock %}

							</article>
					</div>

				</div>
				<div class="col-3 col-12-narrower">
					{{ include('partials/sidebar.html.twig', {'sid': 'sidebar2'}) }}
				</div>
			</div>
		</div>
	</section>
{% endblock %}
